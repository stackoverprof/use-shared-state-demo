{
	"extends": ["../../.eslintrc.base.json"],
	"ignorePatterns": ["!**/*"],
	"overrides": [
		{
			"files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
			"rules": {
				"no-console": "off",
				"no-restricted-syntax": "off"
			}
		},
		{
			"files": ["*.ts", "*.tsx"],
			"rules": {}
		},
		{
			"files": ["*.js", "*.jsx"],
			"rules": {}
		},
		{
			"files": ["*.json"],
			"parser": "jsonc-eslint-parser",
			"rules": {
				"@nx/dependency-checks": [
					"error",
					{
						"ignoredFiles": ["{projectRoot}/vite.config.{js,ts,mjs,mts}"]
					}
				],
				"no-multiple-empty-lines": [
					"warn",
					{
						"max": 1
					}
				]
			}
		}
	]
}
